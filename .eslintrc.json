{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "jest": true,
        "mocha": true
    },
    "rules": {
        "max-len": [
            "error",
            {
                "ignoreComments": true
            }
        ],
        "no-param-reassign":0,
        "no-restricted-syntax": 0
    }
}
